#!/bin/sh
. /etc/default/audibility
export FILE=`curl --user $HF_USER:$HF_PASSWORD -s --ftp-pasv --ftp-skip-pasv-ip --list "$HF_URL" | tail -1`
curl --user $HF_USER:$HF_PASSWORD -s --ftp-pasv --ftp-skip-pasv-ip -o $FILE "$HF_URL/$FILE"
echo $FILE
php /var/www/html/audibility/upload_hf_schedule.php $FILE
rm $FILE
